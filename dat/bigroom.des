$outside = terrain:{ 'L', 'L', '{', '{', 'T', '-' }
SHUFFLE:$outside

REPLACE_TERRAIN:(0,0, 75,20),'.',$outside[0],100%

SWITCH [ 3 ] {
   CASE 0:
     # O
     $sel = selection: complement(gradient(radial, (1 - 5), (6,5), (6,15))) &
     	  	       complement(gradient(radial, (1 - 5), (67,5), (67,15))) &
		       complement(gradient(radial, (1 - 5), (6,5), (67,5))) &
		       complement(gradient(radial, (1 - 5), (6,15), (67,15)))
     BREAK
   CASE 1:
     # H
     $sel = selection: complement(gradient(radial, (1 - 5), (6,5), (6,15))) &
     	  	       complement(gradient(radial, (1 - 5), (67,5), (67,15))) &
		       complement(gradient(radial, (1 - 5), (6,10), (67,10)))
     BREAK
   CASE 2:
     # X
     $sel = selection: complement(gradient(radial, (1 - 5), (6,5), (67,15))) &
     	  	       complement(gradient(radial, (1 - 5), (6,15), (67,5)))
     BREAK
}

IF [ 33% ] {
   # middle bar
   $sel = selection: complement(gradient(radial, (1 - 5), (36,5), (37,15))) & $sel
}

TERRAIN:$sel,'.'

